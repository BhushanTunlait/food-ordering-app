<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:background="@color/white"
android:layout_height="match_parent"
android:clipToPadding="false"
tools:layout_editor_absoluteX="0dp"
tools:layout_editor_absoluteY="0dp">

<ImageView
    android:id="@+id/imgView"
    android:layout_width="122dp"
    android:layout_height="99dp"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="50dp"
    android:adjustViewBounds="true"
    android:gravity="center"
    android:importantForAccessibility="no"
    android:src="@drawable/logo"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.498"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="277dp"
        android:layout_height="85dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/pacifico"
        android:text="Login To Your\nAdmin Dashboard"
        android:textAlignment="center"
        android:textColor="#1F7322"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Ownercontact"
        android:layout_width="match_parent"
        android:layout_height="62dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="44dp"
        android:layout_marginRight="12dp"
        android:autofillHints=""
        android:background="@drawable/edittextshape"
        android:drawableStart="@drawable/mail"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:fontFamily="@font/pacifico"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingStart="10dp"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="HardcodedText,MissingConstraints,RtlSymmetry" />

    <EditText
        android:id="@+id/Password"
        android:layout_width="319dp"
        android:layout_height="62dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/edittextshape"
        android:drawableStart="@drawable/lock"
        android:drawablePadding="15dp"
        android:elevation="5dp"
        android:ems="10"
        android:fontFamily="@font/pacifico"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:paddingStart="10dp"
        app:layout_constraintEnd_toEndOf="@+id/Ownercontact"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Ownercontact"
        app:layout_constraintTop_toBottomOf="@+id/Ownercontact"
        tools:ignore="RtlSymmetry,TextViewEdits,UseCompatTextViewDrawableXml" />

    <ImageButton
        android:id="@+id/showHidePasswordBtn"
        android:layout_width="55dp"
        android:layout_height="41dp"
        android:layout_gravity="center_vertical"
        android:layout_marginEnd="12dp"
        android:background="@android:color/transparent"
        android:paddingEnd="8dp"
        android:src="@drawable/eye"
        app:layout_constraintBottom_toBottomOf="@+id/Password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/Password"
        app:layout_constraintTop_toTopOf="@+id/Password"
        app:layout_constraintVertical_bias="0.424" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/loginbtn"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginTop="116dp"
        android:background="@drawable/greenbuttongradient"
        android:elevation="5dp"
        android:fontFamily="@font/pacifico"
        android:text="@string/login"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/showHidePasswordBtn"
        app:layout_constraintStart_toStartOf="@+id/Password"
        app:layout_constraintTop_toBottomOf="@+id/Password"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textsigh2"
        android:layout_width="191dp"
        android:layout_height="36dp"
        android:layout_marginBottom="16dp"
        android:fontFamily="@font/pacifico"
        android:text="Design By BST"
        android:textAlignment="center"
        android:textColor="#1F7322"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/donthaveacnt"
        app:layout_constraintStart_toStartOf="@+id/donthaveacnt"
        app:layout_constraintTop_toBottomOf="@+id/donthaveacnt"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="HardcodedText,SpUsage" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="332dp"
        android:layout_height="75dp"
        android:fontFamily="@font/pacifico"
        android:text=" Savory Food "
        android:textAlignment="center"
        android:textColor="@color/cardview_dark_background"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imgView"
        app:layout_constraintStart_toStartOf="@+id/imgView"
        app:layout_constraintTop_toBottomOf="@+id/imgView"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/donthaveacnt"
        android:layout_width="191dp"
        android:layout_height="32dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/pacifico"
        android:text="Don't Have An Account?"
        android:textAlignment="center"
        android:textColor="#1F7322"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/loginbtn"
        app:layout_constraintStart_toStartOf="@+id/loginbtn"
        app:layout_constraintTop_toBottomOf="@+id/loginbtn"
        tools:ignore="HardcodedText" />





</androidx.constraintlayout.widget.ConstraintLayout>